@model List<Cinema>


 ﻿<div class="container">
    <div class="row">
@foreach(Cinema cinema in Model)
        {

            <div class="card movie-card" style=" max-width: 400px;">
                <div class="card-body">
                    <a asp-action="GetCinemaDetails" asp-controller="Cinema" asp-route-id="@cinema.Id">@cinema.Name</a>
                </div>
                </div>
        }
</div>


</div>